{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node 23 with TypeScript",
	"_version": "23.0.0",
	"compilerOptions": {
		"lib": ["es2024"],
		"module": "nodenext",
		"target": "es2024",
		"declaration": true,
		"emitDeclarationOnly": true,
		"allowImportingTsExtensions": true,

		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"moduleResolution": "node16"
	}
}
